{
  "name": "film-list",
  "private": true,
  "scripts": {
    "build": "eleventy && tailwindcss -i ./src/tailwind.css -o ./_site/styles.css --minify",
    "prettier:check": "prettier . --check",
    "prettier:write": "prettier . --write",
    "start": "eleventy --serve & tailwindcss -i ./src/tailwind.css -o ./_site/styles.css --watch"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-fetch": "^4.0.1",
    "@shopify/prettier-plugin-liquid": "^1.4.4",
    "lodash": "^4.17.21",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "tailwindcss": "^3.4.3",
    "wink-nlp-utils": "^2.1.0"
  }
}
